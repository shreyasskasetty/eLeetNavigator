{
    "name":"eLeetNavigator",
    "version":"0.1.0",
    "description": "Recommendation System for LeetCode",
    "permissions":["storage","tabs"],
    "host_permissions":["https://leetcode.com/*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": {
        "matches": ["https://leetcode.com/*"],
        "js": ["contentScript.js"]
    }
}